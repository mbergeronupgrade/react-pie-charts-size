{
  "name": "react-pie-charts-size",
  "version": "1.0.0",
  "description": "Measure bundle sizes of the main React pie charts",
  "main": "index.js",
  "dependencies": {
    "@size-limit/preset-small-lib": "^2.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-minimal-pie-chart": "^6.0.0",
    "recharts": "^1.8.5",
    "victory-pie": "^33.1.6"
  },
  "scripts": {
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "src/recharts.js",
      "ignore": [
        "react",
        "react-dom"
      ]
    },
    {
      "path": "src/victoryPie.js",
      "ignore": [
        "react",
        "react-dom"
      ]
    },
    {
      "path": "src/reactMinimalPieChart.js",
      "ignore": [
        "react",
        "react-dom"
      ]
    }
  ],
  "author": "",
  "license": "ISC"
}
